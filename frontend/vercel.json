{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "rewrites": [
    { "source": "/.well-known/skill.md", "destination": "/skill.md" }
  ],
  "headers": [
    {
      "source": "/.well-known/(.*)",
      "headers": [
        { "key": "Content-Type", "value": "text/plain; charset=utf-8" }
      ]
    }
  ]
}
